<main>
  <h1>New expense</h1>
  <%= form_with model: @expense do |form| %>
    <div>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div>
      <%= form.label :price %>
      <%= form.text_field :price %>
    </div>
    <div>
      <%= form.submit %>
    </div>
  <% end %>
</main>
